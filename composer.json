{
  "name": "mittwald/mw-cache-widget",
  "description": "Dashboard widget that displays the current memory usage of OpCodeCache or APCu",
  "require": {
    "php": ">=8.1.0",
    "typo3/cms-core": "^12.4 || ^13.4",
    "typo3/cms-dashboard": "^12.4 || ^13.4"
  },
  "type": "typo3-cms-extension",
  "autoload": {
    "psr-4": {
      "Mittwald\\CacheStatsWidget\\": "Classes"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "mw_cache_widget"
    }
  }
}
